<%= simple_form_for(@wish) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= f.input :user_id %>
    <%= f.input :cost %>
    <%= f.input :catalog_number %>
    <%= f.input :manufacturer %>
    <%= f.input :title %>
    <%= f.input :count %>
    <%= f.input :declared %>
    <%= f.input :average %>
    <%= f.input :country %>
    <%= f.input :session_id %>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
