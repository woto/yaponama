<h1>Listing feed_categories</h1>

<table>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feed_categories.each do |feed_category| %>
  <tr>
    <td><%= feed_category.title %></td>
    <td><%= link_to 'Show', feed_category %></td>
    <td><%= link_to 'Edit', edit_feed_category_path(feed_category) %></td>
    <td><%= link_to 'Destroy', feed_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed category', new_feed_category_path %>
